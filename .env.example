# Copy this file to .env and fill in your actual API keys
# NEVER commit .env to GitHub!

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost/medical_db

# API Keys (Get from respective platforms)
GROQ_API_KEY=your_groq_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Google Cloud (for translation and voice)
GOOGLE_APPLICATION_CREDENTIALS=path/to/google-credentials.json

# Application Settings
SECRET_KEY=your_secret_key_here_generate_with_openssl
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Pathway Configuration
PATHWAY_LICENSE_KEY=your_pathway_license_if_needed

# Optional: External Services
REDIS_URL=redis://localhost:6379/0
